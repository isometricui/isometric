{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

  "rushVersion": "5.45.4",
  "pnpmVersion": "6.2.1",

  "pnpmOptions": {
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0",
  "gitPolicy": {},

  "repository": {
    "url": "https://github.com/isometricui/new-isometric",
    "defaultBranch": "main"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@isometric/eslint-config",
      "projectFolder": "config/eslint-config",
      "reviewCategory": "config"
    },
    {
      "packageName": "@isometric/tsconfig",
      "projectFolder": "config/tsconfig",
      "reviewCategory": "config"
    },
    {
      "packageName": "@isometric/build",
      "projectFolder": "utilities/build",
      "reviewCategory": "utility"
    }
  ]
}
